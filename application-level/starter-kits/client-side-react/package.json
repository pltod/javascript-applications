{
  "name": "client-side-react",
  "version": "1.0.0",
  "description": "Client side app with React View Layer",
  "scripts": {
    "build-dev": "watchify app/boot.js -o public/build.js --debug --verbose",
    "build-prod": "browserify app/boot.js | uglifyjs > public/build.min.js",
    "test": "node app/test/main.js | colortape"
  },
  "keywords": [
    "client-side",
    "react",
    "browserify"
  ],
  "author": "@pltod",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
